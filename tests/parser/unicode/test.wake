def testBool _ =
    require True =
        "\u0073\u0323\u0307" ==* "\u1e69"
    require True =
        "ṩ" ==* "ṩ"
    require True =
        "ṩ{"-"}ṩ" ==* "ṩ-ṩ"
    require False =
        'ṩ' ==* 'ṩ'
    def xṩ =
        44
    def _ =
        xṩ
    require False =
        "xϓ" ==* "xΎ"
    require False =
        unicodeIdentifier "xϓ" ==* "xΎ"
    def xϓ =
        33
    def _ =
        xΎ
    True


def test x =
    if testBool x then
        Pass Unit
    else
        Fail (makeError "normalization bad")


