{
  "label": "postgres command",
  "environment": [
    "PATH=/usr/lib64/ccache:/usr/bin:/usr/sbin:/bin:/sifive/tools/rust/stable/1.72.0/bin",
    "TERM=xterm-256color",
    "DATA_DIR=/tmp/pg_data",
    "PG_DIR=/scratch/jakee/pg-server2"
  ],
  "command": ["bash"],
  "user-id": 0,
  "group-id": 0,
  "visible": [],
  "directory": ".",
  "stdin": ".build/postgres-cargo.sh",
  "resources": [],
  "isolate-network": true,
  "isolate-pids": true,
  "mount-ops": [
    {"type": "tmpfs", "destination": "/tmp"},
    {"type": "tmpfs", "destination": "/root"},
    {
      "type": "create-dir",
      "destination": "rust/rsc/.cargo"
    },
    {
      "type": "create-file",
      "destination": "rust/rsc/.cargo/config.toml"
    },

    {
      "type": "bind",
      "source": "rust/rsc/vendor-config.toml",
      "destination": "rust/rsc/.cargo/config.toml"
    },
  ],
  "usage": {
    "status": 0,
    "runtime": 0,
    "cputime": 0,
    "membytes": 0,
    "inbytes": 0,
    "outbytes": 0
  }
}